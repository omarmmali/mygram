<html>
	<head>
	</head>
	<body>
		<% @posts.each do |post| %>
		<div class = 'posts-wrapper'>
			<div class = 'row'>
				<div class = 'post-head'>
					<div class = 'name'>
						Omar
					</div>
				</div>
			<div class = 'image-center-block'>
				<%= link_to (image_tag post.image.url(:medium)),post_path(post) %>
			</div>
		<p class = 'caption'><%= post.caption %></p>
			</div>
		</div>
		<% end %>
	</body>
</html>